#include<bits/stdc++.h>
using namespace std;
int main()
{
	int x, y, z;
		cout <<"ba so cua ban :";
		cin >>x>>y>>z;
	if(x*x==y*y+z*z)
    { 
		cout <<"la tam giac vuong "; 
		return 0;
	}
	if(y*y==x*x+z*z)
	{
		cout <<"la tam giac vuong ";
	    return 0;
	}
	if(z*z==x*x+y*y)
	{
		cout <<" la tam giac vuong ";
		return 0;
	}
	cout <<"la tam giac thuong ";
		return 0;
    
}
