#include<stdio.h>
#include<string.h>
#define space " "
int main () {
	char a[100];
	gets(a);
	char *p;
	strlwr(a);
	p = strtok(a ,space);
	while(p!=NULL){

		p = strtok(NULL,space);
	}
}









